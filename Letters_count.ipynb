        "string = \"hippo runs to us!\"\n",
        "word_dict = {}\n",
        "for i in string :\n",
        "\n",
        "  if i in word_dict :\n",
        "    word_dict[i] += 1\n",
        "\n",
        "  else :\n",
        "    word_dict[i] = 1\n",
        "\n",
        "print(word_dict)\n",
        "\n",
        "\n"
